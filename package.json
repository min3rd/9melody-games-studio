  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.33",
    "prisma": "^5.9.0",
    "ts-node": "^10.9.1",
    "tailwindcss": "^4.2.0",
    "typescript": "^5.3.0",
    "vitest": "^1.3.0"
